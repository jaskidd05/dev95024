&lt;script&gt;&lt;![CDATA[(function() {
	
	data.selection = {'displayValue':'','value':''};
 
	if(input.hasOwnProperty('getChart')){
		
		var today = new GlideDateTime().getNumericValue();//we'll provide epoch default dates in case no dates are found in an empty sprint
		var chartData = {"categories":[], "series":[], "rawData":[], "states":{"New":0}, "dates":{}, "start":today, "end":today + (86400000 * 7)}
		var eachDate, eachState
 
		var thisTable = 'rm_story'
		var thisSprint = input.getChart;
		
		//First get all the possible story states
		var theseStates = new GlideRecord('sys_choice');
		theseStates.addEncodedQuery('element=state^name=rm_story');
		theseStates.query();
		while(theseStates.next()){
			chartData.states[theseStates.label] = 0;
		} 

		//Second, get the date ranges covered by this sprint
		var sprintDates =new GlideAggregate('rm_story');
		sprintDates.addEncodedQuery('sprint.sys_id='+thisSprint)
		sprintDates.addAggregate('MIN','sys_created_on');
		sprintDates.addAggregate('MAX','sys_updated_on');
		sprintDates.groupBy('sprint');
		sprintDates.query();
		if(sprintDates.next()){
			chartData.start = new GlideDateTime(sprintDates.getAggregate('MIN','sys_created_on')).getNumericValue();
			chartData.end   = new GlideDateTime(sprintDates.getAggregate('MAX','sys_updated_on')).getNumericValue();
		}

		chartData.end+= (86400000 * 1)
		
		//if(chartData.start==chartData.end){chartData.end+= (86400000 * 7)}

		//Third, calculate all the dates between the start and end
		eachDate = chartData.start;

		while (eachDate &lt;= chartData.end) {
			chartData.dates[convertDate(new Date(eachDate))] = true;
			eachDate += 86400000;
		}

		//Fourth, now we can populate the X &amp; Y coordinates with every possibility of state and date
		for(eachDate in chartData.dates){
			chartData.rawData.push({"x": eachDate, "y": JSON.parse(JSON.stringify(chartData.states))})
		}

		//Fifth step, we can populate our raw data with values from the sprint
		var stories = new GlideRecord('rm_story')
		stories.addEncodedQuery('sys_class_name=rm_story^sprint='+thisSprint)
		stories.query();

		while(stories.next()){
			var thisSysID = stories.sys_id.toString();

			var setSysid = new GlideHistorySet('rm_story', thisSysID).generate();

			var historyLines =new GlideRecord('sys_history_line');
			historyLines.addEncodedQuery('set.sys_id='+setSysid+'^label=state');
			historyLines.query();

			while(historyLines.next()){

				eachDate = historyLines.update_time.toString().substring(0,10);
				//Match the date from the history audit with our raw data
				chartData.rawData.forEach(function(occassion){
					if(occassion.x == eachDate){
						var from = gs.nil(historyLines.old.toString()) ? 'New': historyLines.old.toString();
						var to = historyLines['new'].toString();
						occassion.y[from]--;
						occassion.y[to]++;
					}
				})
			}
		}

		//This has given us the activity on each day, now we want to push that into rolling totals accumulating throughout the sprint

		//Sixth step, we can now accumulate values/movement from one day to the next
		chartData.rawData.forEach(function(occassion){
			for(eachState in occassion.y){
				chartData.states[eachState]= chartData.states[eachState] + occassion.y[eachState];
				occassion.y[eachState] = chartData.states[eachState];
			}		
		})

		//Because "New" isn't actually a state, it ends up in the negative so we'll switch this to positive (also, note that new NEVER goes down as we're tracking something slightly different and that's how many &amp; when new stories appear)
		chartData.rawData.forEach(function(occassion){
			occassion.y['New'] = 0 - occassion.y['New'];
		})

		////////////////////////////////////////////////////////////////////////////
		//Finally, we can massage the data to suit Highcharts
		for(eachDate in chartData.dates){chartData.categories.push(eachDate)}

		for(eachState in chartData.states){
			var stateData = [];
			chartData.rawData.forEach(function(occassion){
				stateData.push(occassion.y[eachState])
			})
			chartData.series.push({"name": eachState, 
														 "data":stateData, 
														 "type":"areaspline", 
														 "fillOpacity":eachState=="New" ? "0" :"0.3"
														 
														})
		}
		
	  data.chartData = chartData
		console.log(data.chartData) 
	}	


	///////////////////////////////////////////////
	function convertDate(date) {
		var yyyy = date.getFullYear().toString();
		var mm = (date.getMonth()+1).toString();
		var dd  = date.getDate().toString();

		var mmChars = mm.split('');
		var ddChars = dd.split('');

		return yyyy + '-' + (mmChars[1]?mm:"0"+mmChars[0]) + '-' + (ddChars[1]?dd:"0"+ddChars[0]);
	}
	///////////////////////////////////////////////



})();]]&gt;&lt;/script&gt;
