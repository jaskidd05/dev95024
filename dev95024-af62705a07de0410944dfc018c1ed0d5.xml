&lt;client_script&gt;&lt;![CDATA[function($scope) {

	var c = this, thisChart;
	$scope.showInstructions = true;

	$scope.selectedSprint = {
		displayValue: c.data.selection.displayValue,
		value: c.data.selection.value,
		name: 'selectedSprint'
	};

	$scope.$on("field.change", function(evt, parms) {
		if (parms.field.name == 'selectedSprint'){

			c.server.get({getChart:parms.newValue}).then(function(response){
			
				$scope.showInstructions = false;
				
				c.data = response.data;

				if(thisChart!==undefined){thisChart.destroy();}

				thisChart = new Highcharts.chart('container', {
					chart: {
						type: 'areaspline'
					},
					title: {
						text: parms.displayValue
					},
					subtitle: {
						text: 'Sprint Performance'
					},
					xAxis: {
						
						categories: c.data.chartData.categories /*,
					plotBands: [{ // visualize the weekend
							from: 4.5,
							to: 6.5,
							color: 'rgba(68, 170, 213, .2)'
					}]*/
					},
					yAxis: {
						allowDecimals: false,
						title: {
