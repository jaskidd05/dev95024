					},
					credits: {
						enabled: false
					},
					plotOptions: {
						areaspline: {
							marker: {
								enabled: false
							}
						}
					},
					series: c.data.chartData.series
				});
			})
		}
	})
}]]&gt;&lt;/client_script&gt;
